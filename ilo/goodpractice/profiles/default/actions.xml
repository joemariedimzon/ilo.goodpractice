<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
 xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="object_buttons" meta_type="CMF Action Category">

  <object name="Export CSV" meta_type="CMF Action" i18n:domain="ilo.goodpractice">
   <property name="title" i18n:translate="">Export CSV</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${object/aq_parent/absolute_url}/@@collective.excelexport</property>
   <property name="available_expr">object/@@faceted_subtyper/can_enable</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="visible">True</property>
  </object>

  <object name="Export CSV (Faceted Navigation)" meta_type="CMF Action" i18n:domain="ilo.goodpractice">
   <property name="title" i18n:translate="">Export CSV (Faceted Navigation)</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">object/eea_excel_export_url</property>
   <property name="available_expr">object/@@faceted_subtyper/can_disable</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="visible">True</property>
  </object>

 </object>
</object>
